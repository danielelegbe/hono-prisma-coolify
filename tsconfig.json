{
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "esModuleInterop": true,
    "baseUrl": "./src",
    "strictNullChecks": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "target": "ESNext",
    "moduleResolution": "node",
    "skipLibCheck": true
  },
    "exclude": ["node_modules"],
    "include": ["src/**/*"]
}
